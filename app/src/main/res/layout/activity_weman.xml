<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_weman"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#7fa87f"
    tools:context="com.example.fatma.fainalproject1.weman">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/famale"
        android:id="@+id/imageView3"
        android:layout_below="@+id/button10"
        android:layout_alignStart="@+id/imageView"
        android:layout_marginTop="17dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/famale"
        android:id="@+id/imageView5"
        android:layout_below="@+id/button12"
        android:layout_alignStart="@+id/imageView3"
        android:layout_marginTop="20dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/famale"
        android:id="@+id/imageView6"
        android:layout_alignTop="@+id/imageView5"
        android:layout_alignStart="@+id/imageView2" />

    <Button
        android:text="NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button11"
        android:background="#7fa87f"
        android:layout_marginEnd="53dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentEnd="true"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/famale"
        android:id="@+id/imageView2"
        android:layout_above="@+id/button10"
        android:layout_alignEnd="@+id/button11"
        android:layout_marginEnd="19dp" />

    <Button
        android:text="NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:background="#7fa87f"
        android:layout_below="@+id/imageView4"
        android:layout_alignStart="@+id/button11" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/famale"
        android:id="@+id/imageView4"
        android:layout_above="@+id/button12"
        android:layout_alignStart="@+id/imageView6" />

    <Button
        android:text="NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button15"
        android:background="#7fa87f"
        android:layout_below="@+id/imageView6"
        android:layout_alignStart="@+id/button13" />

    <Button
        android:text="NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button14"
        android:background="#7fa87f"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="26dp" />

    <Button
        android:text="NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button10"
        android:background="#7fa87f"
        android:layout_below="@+id/imageView"
        android:layout_alignStart="@+id/button14" />

    <Button
        android:text="NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:background="#7fa87f"
        android:layout_below="@+id/imageView3"
        android:layout_alignStart="@+id/button10" />

    <TextView
        android:text="Select Trainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:id="@+id/textView3"
        android:textSize="40sp"
        android:textColor="@android:color/white"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/famale"
        android:layout_marginTop="34dp"
        android:id="@+id/imageView"
        android:adjustViewBounds="false"
        android:maxWidth="?android:attr/windowTitleSize"
        android:layout_below="@+id/textView3"
        android:layout_alignEnd="@+id/button10"
        android:layout_marginEnd="18dp" />

    <SearchView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:background="@drawable/common_google_signin_btn_icon_dark"
        android:layout_alignParentTop="true" />

</RelativeLayout>
